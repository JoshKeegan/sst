# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[Makefile]
indent_style = tab

[{*.yml,*.yaml}]
indent_style = space
indent_size = 2

[*.xml]
indent_style = space
indent_size = 2

[{*.js,*.json}]
indent_style = space
indent_size = 4

[package.json]
indent_size = 2
